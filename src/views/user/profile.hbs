    <div class="d-flex align-items-center">

      <input
        type="text"
        class="form-control"
        id="username"
        name="username"
        disabled
        value="{{this.userData.username}}"
      />
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="16"
        height="16"
        fill="currentColor"
        class="bi bi-pencil-square"
        viewBox="0 0 16 16"
        style="cursor: pointer;"
      >
